   "source": [
    "from collections import Counter\n",
    "\n",
    "x = int(input())\n",
    "lista = list(map(int, input().split()))\n",
    "n = int(input())\n",
    "\n",
    "acum = 0\n",
    "for i in range(n):\n",
    "    n, m = map(int,input().split())\n",
    "    \n",
    "    if n in Counter(lista).keys():\n",
    "        acum += m\n",
    "        lista.remove(n)\n",
    "\n",
    "print (acum)\n"
   ]
