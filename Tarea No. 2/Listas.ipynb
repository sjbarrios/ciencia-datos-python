    "if __name__ == '__main__':\n",
    "    n = int(input())\n",
    "    arr = list(map(int, input().split()))\n",
    "    \n",
    "    maximo = max(arr)\n",
    "    minimo = min(arr)\n",
    "    \n",
    "    for i in range(n):\n",
    "        if arr[i] == maximo:\n",
    "            arr[i] = minimo\n",
    "    print (max(arr))"
