    "def is_leap(year):\n",
    "    leap = False\n",
    "\n",
    "    if (year%4 == 0):\n",
    "        leap = True\n",
    "        if (year%100 == 0):\n",
    "            leap = False\n",
    "            if(year%400 == 0):\n",
    "                leap = True\n",
    "    return leap\n",
    "\n",
    "year = int(input())\n",
    "is_leap(year)"
